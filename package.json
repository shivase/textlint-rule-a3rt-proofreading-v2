{
  "name": "textlint-rule-a3rt-proofreading-v2",
  "version": "0.1.0",
  "main": "dist/index.js",
  "description": "textlint rule to check Japanese with A3RT Proofreading API",
  "repository": "https://github.com/shivase/textlint-rule-a3rt-proofreading-v2",
  "author": "shivase",
  "license": "MIT",
  "keywords": [
    "textlint",
